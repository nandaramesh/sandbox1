
<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Department</th>
    <th>Hospital</th>
    <th>Description</th>
  </tr>

<% @doctors.each do |doctor| %>
  <tr>
    <td><%= doctor.first_name %></td>
    <td><%= doctor.last_name %></td>
    <td><%= doctor.department %></td>
    <td><%= doctor.hospital %></td>
    <td><%= doctor.description %></td>
  </tr>
<% end %>
</table>

<br />

